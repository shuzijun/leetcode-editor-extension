{
  "name": "leetcode-editor-extension",
  "displayName": "Leetcode editor extension",
  "version": "0.0.2",
  "description": "Quickly open problems in IntelliJ Platform IDEs.",
  "author": "shuzijun",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jetbrains/logos": "^2.2.7",
    "@plasmohq/storage": "^0.12.2",
    "antd": "^4.23.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.5.4",
    "@testing-library/dom": "8.19.0",
    "@types/chrome": "0.0.197",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "plasmo": "0.57.1",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "manifest": {
    "default_locale": "en",
    "host_permissions": [
      "https://leetcode.cn/*",
      "https://leetcode.com/*",
      "https://codetop.cc/*"
    ],
    "permissions": []
  }
}
